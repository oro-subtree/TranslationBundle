{% block genemu_jqueryselect2_translatable_entity_widget %}
    {% spaceless %}
        {{ block('genemu_jqueryselect2_entity_widget') }}
    {% endspaceless %}
{% endblock %}

{#
    This form field are rendered on System configuration form
#}
{% block oro_translation_available_translations_widget %}
    {{ form_widget(form) }}
    <div style="position: relative" class="available-translation-widget-container">
        {{ oro_widget_render({
            'widgetType': 'block',
            'url': path('oro_translation_available_translations'),
            'elementFirst': false,
            'loadingElement': '.available-translation-widget-container'
        }) }}
    </div>
{% endblock %}
